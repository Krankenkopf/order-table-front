.preloader
  position absolute
  margin auto
  //width 100%
  //height 100%
  top 50%
  left 50%
  transform: translate(-50%, -50%)
  & > * 
    animation: spin-slip infinite 2s 0.5s running;

@keyframes spin-slip {
  0% {
    transform: rotate(0deg);
  }
  7% {
    transform: rotate(-25deg);
  }
  14% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(720deg);
  }
}